{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "anhskohbo/no-captcha": "^3.0",
        "barryvdh/laravel-elfinder": "^0.4.2",
        "bumbummen99/shoppingcart": "^2.8",
        "fideloper/proxy": "^4.0",
        "froala/wysiwyg-editor-php-sdk": "1.0.1",
        "intervention/image": "^2.4",
        "langnonymous/lang": "^1.2",
        "laravel/cashier": "~7.0",
        "laravel/framework": "5.8.*",
        "laravel/telescope": "^2.0",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.4.0",
        "mkhdev/awt": "^1.0",
        "paypal/rest-api-sdk-php": "*",
        "phpanonymous/it": "^1.0",
        "symfony/dotenv": "^4.3",
        "weboap/visitor": "^2.0",
        "willvincent/laravel-rateable": "^1.07",
        "yajra/laravel-datatables": "*",
        "yoeunes/toastr": "^1.2"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories",
            "database/seeds/User.php",
            "database/seeds/Partner.php",
            "database/seeds/Tag.php",
            "database/seeds/PostDB.php",
            "database/seeds/Slider.php",
            "database/seeds/User_role.php",
            "database/seeds/Service.php"
        ],
        "files":[
            "app/Http/helper.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
